{
  "name": "music-visualization-canvas",
  "version": "1.0.1",
  "description": "music visualization",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "jsnext:main": "lib/index.es.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "dev": "parcel ./src/index.html --open",
    "build": "rollup -c",
    "build:example": "rm -rf dist/* && parcel build ./src/index.html --public-url ./ --out-dir dist --no-source-maps",
    "deploy": "bash build/deploy.sh",
    "release": "bash build/release.sh"
  },
  "keywords": [
    "music",
    "canvas"
  ],
  "author": "mengxiong10",
  "license": "MIT",
  "repository": "mengxiong10/music-visualization-canvas",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "cssnano": "^4.1.8",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^4.1.0",
    "gh-pages": "^2.0.1",
    "parcel-bundler": "^1.12.3",
    "rollup": "^1.18.0",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript2": "^0.22.1",
    "typescript": "^3.5.3"
  }
}
